{
    "version": 2,
    "name": "my-portfolio-backend",
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/server.ts"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "functions": {
        "src/server.ts": {
            "maxDuration": 30
        }
    }
}